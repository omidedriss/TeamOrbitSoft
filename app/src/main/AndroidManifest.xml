<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.orbitsoft.teamorbitsoft">
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TeamOrbitSoft"
        tools:targetApi="31">
        <activity
            android:name=".example.IntentSample"
            android:exported="false" />
        <activity
            android:name=".example.LifeCycleActivity"
            android:exported="false" />
        <activity
            android:name=".Saeedian.Saaedian"
            android:exported="false" />
        <activity
            android:name=".Hatami.Hatami"
            android:exported="false" />
        <activity
            android:name=".Moosavi.Moosavi"
            android:exported="false" />
        <activity
            android:name=".Gorji.Gorji"
            android:exported="false" />
        <activity
            android:name=".Nikkhu.Nikkhu"
            android:exported="false" />
        <activity
            android:name=".Kanani.Kanani"
            android:exported="false" />
        <activity
            android:name=".Mazaheri.Mazaheri"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".example.MyBrowserActivity"
            android:label="@string/app_name"
            android:exported="true">
            <intent-filter tools:ignore="AppLinkUrlError">
                <action android:name="android.intent.action.VIEW" />
                <action android:name="com.orbitsoft.teamorbitsoft.MyBrowser" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="com.orbitsoft.teamorbitsoft.Apps" />
                <category android:name="com.orbitsoft.teamorbitsoft.OtherApps" />
                <data android:scheme="http" />
            </intent-filter>
        </activity>
    </application>

</manifest>